{
  "name": "",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "node build.js && remix build",
    "dev": "node build.js && CHOKIDAR_USEPOLLING=true remix dev --manual -c \"npm run dev:server\"",
    "dev:server": "tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./server.ts",
    "start": "netlify serve",
    "test": "jest",
    "test:all": "RUN_INFRA_TESTS=true FIREBASE_API_KEY=TEST_FIREBASE_API_KEY DATABASE_URL=TEST_DATABASE_URL jest --runInBand",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@netlify/edge-functions": "^2.0.0",
    "@netlify/remix-edge-adapter": "^3.0.0",
    "@netlify/remix-runtime": "^2.0.0",
    "@remix-run/css-bundle": "^2.4.0",
    "@remix-run/express": "^2.4.0",
    "@remix-run/node": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@remix-run/dev": "^2.4.0",
    "@remix-run/eslint-config": "^2.4.0",
    "@remix-run/serve": "^2.4.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/source-map-support": "^0.5.6",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
